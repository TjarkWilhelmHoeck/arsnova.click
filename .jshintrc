{
  "browser": true,
  "esnext": true,
  "undef": true,
  "unused": true,
  "curly": true,
  "eqnull": true,
  "forin": true,
  "latedef": true,
  "noarg": true,
  "nonbsp": true,
  "globals": {
    "_": false,
    "$": false,
    "hljs": false,
    "marked": false,
    "MathJax": false,
    "buzz": false,
    "Router": false,
    "ReactiveCountdown": false,
    "App": false,
    "console": false,
